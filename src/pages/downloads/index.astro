---
import Layout from "@/layouts/Layout.astro";
import ComponentCollectionMapper from "@/components/ComponentCollectionMapper.vue";
import downloads from "@/content/downloads.json";

const data = downloads?.data?.body || {};
const seoData = downloads?.data?.seo || {};
---

<Layout metaObj={seoData}>
  <ComponentCollectionMapper data={data} client:load />
</Layout>
