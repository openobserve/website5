---
import Layout from "../layouts/Layout.astro";
import ComponentCollectionMapper from "../components/ComponentCollectionMapper.vue";
import fetchApi from "../utils/strapi";
import CommonFormWrapper from "@/components/wrapper/CommonFormWrapper.vue";
import { getCaseStudies } from "@/utils/cache";

const PageData = await fetchApi({
  endpoint: "api/homepage",
  query: { pLevel: "10", status: "draft" },
});

const loadcaseStudyData = getCaseStudies();
---

<Layout>
  <!-- <CustomAnnouncement AnnouncementBanner={announcementBanner} /> -->
  <ComponentCollectionMapper
    data={PageData.data.body}
    caseStudyData={}
    client:load
  />
</Layout>
