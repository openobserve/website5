---
import ComponentCollectionMapper from "@/components/ComponentCollectionMapper.vue";
import Layout from "@/layouts/Layout.astro";
import policies from "@/content/privacy-policies.json";
const metaObj = {
  title:
    "Privacy Policy | OpenObserve",
    description:"Learn how we collect, use, and share your personal information. This page details our privacy practices, including how we protect your data and your rights."
};
const data = policies?.data?.body || {};
---

<Layout metaObj={metaObj}>
  <ComponentCollectionMapper data={data} client:load />
</Layout>
